<div class="con-main" id="merGroupUserDetailView" style="display:none;">
	<h3 class="tit-h3">人员详情</h3>
	<div class="com-bor-box com-bor-box01">
		<form action="/">
			<table class="base-table">
				<col width="140" />
				<col width="330" />
				<col width="120" />
				<tr>
					<th>部门名称：</th>
					<td><span id="depNameSpan"></span></td>
					<th>姓名：</th>
					<td><span id="gpUserNameSpan"></span></td>
				</tr>
				<tr>
					<th>公交卡号：</th>
					<td><span id="cardCodeSpan"></span></td>
					<th>卡类型：</th>
					<td><span id="cardTypeSpan"></span></td>
				</tr>
				<tr>
					<th>充值金额（元）：</th>
					<td><span id="rechargeAmountSpan"></span></td>
					<th>充值方式：</th>
					<td><span id="rechargeWaySpan"></span></td>
				</tr>
				<tr>
					<th>手机号码：</th>
					<td><span id="mobiltleSpan"></span></td>
					<th>电话：</th>
					<td><span id="phoneSpan"></span></td>
				</tr>
				<tr>
					<th>身份证号：</th>
					<td><span id="identityNumSpan"></span></td>
					<th>入职日期：</th>
					<td><span id="employeeDateSpan"></span></td>
				</tr>
				<tr>
					<th>备注：</th>
					<td style="word-break:break-all"><span id="remarkSpan" class="remark"></span></td>
					<th>状态：</th>
					<td><span id="empTypeSpan"></span></td>
				</tr>
				<tr>
					<td colspan="4" class="a-center">
						<div class="btn-box">
						<a href="#" class="orange-btn-h32" onclick="cancelView('merGroupUser');">返回</a>
						</div>
					</td>
				</tr>
			</table>
		</form>
	</div>
</div>
